{% extends 'base.html.twig' %}

{% block title %}{% trans %}InstructorCourse{% endtrans %}{% endblock %}
{% block breadcrumbhome %}
    <a href="{{ path('instructor_course_index') }} ">
	   {% trans %}Exam{% endtrans %} 
	</a> 
{% endblock %}
{% block breadcrumbcontent %}{% trans %}Show{% endtrans %}{% endblock %}
{% block body %}
    <h1>{% trans %}InstructorCourse{% endtrans %}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ instructor_course.id }}</td>
            </tr>
            <tr>
                <th>{% trans %}Date{% endtrans %}</th>
                <td>{{ instructor_course.date ? instructor_course.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>{% trans %}Active{% endtrans %}</th>
                <td>{{ instructor_course.active ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('instructor_course_index') }}">{% trans %}Back to list{% endtrans %}</a>

    <a href="{{ path('instructor_course_edit', {'id': instructor_course.id}) }}">{% trans %}Edit{% endtrans %}</a>

    {{ include('instructor_course/_delete_form.html.twig') }}
{% endblock %}
