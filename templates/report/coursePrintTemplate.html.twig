<html>
	<style>
		nav {
			height: 60px;
			background: #0d375a;
            }
		html,
		body {
			padding: 0;
			margin: 0;
		}

		.container {
			padding: 30px
		}

	</style>
	<body>
		<nav>
            <img height="50px;" style="padding:5px 10px;float:right" src="data:image/jpeg;base64,">
            <img height="52px" style="padding:10px 10px;vertical-align:middle" src="data:image/jpeg;base64,">

            <span style="color:white;vertical-align:middle;margin-left: 150px"> African Artificial Inteligence Research Center</span>
        </nav>
        <center>
            <h4>Course Information</h4>
        </center>
        <hr>
		<div class="container" style="margin-top:-15px">
			<h4>Course Name</h4>
            <p>Category: {{course.category.name}}</p>
			<span>
				{{course.name}}
			</span>
			{% if course.description %}

				<h4>Course Description</h4>
				<p>
					{{ course.description }}</p>
			{% endif %}
			{% if total_student %}
				<h4>
					Total Number of students taking the course</h4>
				<p>{{total_student}}</p>
			{% endif %}
			{% if instructor_info %}
				<h4>
					Instructor Infromation</h4>
				<table>
					<tr>
						<td>Name : </td>
						<td>
                            {{instructor_info.instructor.user.firstName}}
                            {{instructor_info.instructor.user.middleName}}
                            {{instructor_info.instructor.user.lastName}}
						</td>
                    </tr>
                    <tr>
						<td>Email : </td>
						<td>{{instructor_info.instructor.user.email}}</td>
					</tr>
				</table>
			{% endif %}

            {% if content_view %}
			    <h4>Chapters and Contents</h4>
            {% else %}
                <h4>Chapters</h4>         
            {% endif %}

			{% if contents %}
				{% set title = "" %}
				{% for content in contents %}
					<dl style="margin-left: 30px;">
						{% if title != content.chapter.topic %}
							<dt style="margin-top: -9px;">
								<p>
									<b>{{loop.index}}
										.
										{{content.chapter.topic}}</b>
								</p>
							</dt>
							{% set title = content.chapter.topic %}
						{% endif %}
						{% if content_view %}
							<dd>
								<li>{{content.title}}</li>
							</dd>
						{% endif %}
					</dl>
				{% endfor %}
			{% else %}
				<li style="list-style:none">No content found</li>
			{% endif %}

		</div>
	</body>

</html>
