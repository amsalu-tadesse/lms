{% extends 'sBase.html.twig' %}


{% block stylesheets %}
<style> 
body {
  margin: 0;
}
iframe {
  height:calc(100vh - 150px) !important;
  width:calc(100vw - 250px);
  /*box-sizing: border-box;*/
}
 .card  {
  height:calc(100vh - 150px);
  width:calc(100vw - 250px);
  /*box-sizing: border-box;*/
}
 
</style>
{% endblock %}



{% block title %}Course Request index{% endblock %}
{% block breadcrumbhome %}Course Request{% endblock %}
{% block breadcrumbcontent %}index{% endblock %}

{% block body %}
<br>
<div class="card card-info">
    <div class="card card-header">
        <div class="row">
            <div class="col-md-6">
                <h3 class="card-title">Request list</h3>
            </div>
        </div>
    </div>
    <div class="card card-body"  >
       
 <iframe  src="https://www.youtube.com/embed/{{arg}}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen style="height:500px"></iframe>  
    </div>
</div>

{% endblock %}
{% block javascript %}
    <script>    
        $(window).on('load', function () {
            $(".breadcrumb").parent().parent().remove();
        });
    </script>
{% endblock %}