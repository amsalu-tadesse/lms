<form method="post" action="{{ path('go_live_delete', {'id': go_live.id}) }}" onsubmit="return confirm('{{ "Are you sure you want to delete this item?" | trans | raw}}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ go_live.id) }}">
    <button class="btn btn-danger">{% trans %}Delete{% endtrans %}</button>
</form>
