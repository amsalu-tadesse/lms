	<body class="hold-transition sidebar-mini layout-fixe">


    <div class="wrapper">

  <!-- Preloader -->
  <div class="preloader flex-column justify-content-center align-items-center">
    <img class="animation__shake" src="dist/img/AdminLTELogo.png" alt="AdminLTELogo" height="60" width="60">
  </div>

  
{% include 'master/navbar.html.twig' %}
{% include 'master/sidebar.html.twig' %}
  

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6"> 
            <h1 class="m-0">ECA Learning management system</h1>
          </div> 
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">
			{% block breadcrumbhome %}
			 Home
			{% endblock %}
			 
			  </a></li>
              <li class="breadcrumb-item active">
			  {% block breadcrumbcontent %}
			 Content
			{% endblock %}
			  </li>
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <section class="content">
  
				<div class="row">
					<div class="col-md-12">

						{% for label, messages in app.flashes %}
							{% for message in messages %}
								<div class="row alert alert-{{ label }} alert-dismissible">
									<button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
									<h4 class="mr-4">
										<i class="icon fa fa-{% if label=='success' %}check {% else %}ban {% endif %}"></i>
										Alert!</h4>
									{{ message }}

								</div>


							{% endfor %}
						{% endfor %}

					</div>
				</div>
				<div class="container-fluid">
					<div
						class="row">
						<!-- left column -->
						<div class="col-md-12">
							{% block body %}
							 
							{% endblock %}
						</div>
						<!--/.col (left) -->
						<!-- right column -->
						<div class="col-md-6"></div>
						<!--/.col (right) -->
					</div>
					<!-- /.row -->
				</div>
				<!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->






		 





